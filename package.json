{
  "name": "cognimates-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "npm run prod-node-server",
    "prestart": "npm run sass-build",
    "dev": "concurrently \"npm:sass-watch\" \"npm:dev-node-server\"",
    "prod-node-server": "node index.js",
    "dev-node-server": "nodemon index.js --http",
    "sass-build": "node-sass scss/style.scss -o static/css/",
    "sass-watch": "node-sass --watch scss/style.scss -o static/css/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^2.1.1",
    "axios": "^1.7.2",
    "body-parser": "^1.18.2",
    "clarifai": "^2.9.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "csv": "^6.3.9",
    "ejs": "^3.1.10",
    "express": "^4.16.3",
    "express-busboy": "^7.0.0",
    "express-handlebars": "^7.1.3",
    "localforage": "^1.7.3",
    "minimist": "^1.2.0",
    "node-sass": "^9.0.0",
    "whirlpool": "0.0.4"
  },
  "devDependencies": {
    "mkdirp": "^3.0.1",
    "nodemon": "^3.1.4"
  }
}
